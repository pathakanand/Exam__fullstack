<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
{{> navbar}}
<body>
    <h1>Blogpage</h1>
    
    {{#each blogs}}
    <ul class="blog">
        <li>{{this.name}}</li>
        <li>{{this.Class}}</li>
        <li>{{this.blog}}</li>
        <li>{{this.brand}}</li>
        <li>{{this.price}}</li>
        <li>{{this.stamp}}</li>
        <button><a href="/delete/{{this.blogId}}">Delete</a></button>
        <button><a href="/update/{{this.blogId}}">Update</a></button>
    </ul>
    {{/each}}
</body>

</html>